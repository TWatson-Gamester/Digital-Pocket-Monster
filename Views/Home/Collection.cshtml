@model IEnumerable<Card>

<div class="row justify-content-center">
    <h1>Collection</h1>
    <h1>List of Cards</h1>
</div>
<div class="row">
    <nav class="col-md-2 sidebar gy-5">
        <div class="sidebar-sticky">
            <ul class="nav flex-column">
                <li class="nav-item">HI</li>
                <li class="nav-item">HI</li>
                <li class="nav-item">HI</li>
                <li class="nav-item">HI</li>
                <li class="nav-item">HI</li>
            </ul>
        </div>
    </nav>
    @if (Model == null)
    {
        <h1>no cards</h1>
    }
    else
    {
        foreach (var card in Model)
        {
            <div class="col-md-9 ml-sm-auto col-lg-10 px-4">-
                <div class="round-10 text-center btn-neum-black bold">
                    <div>
                        <p><partial name="_CardPartialView.cshtml" for="@card" /> </p>
                    </div>
                </div>
            </div>
        }
    }
</div>
